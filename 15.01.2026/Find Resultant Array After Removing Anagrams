class Solution(object):
    def removeAnagrams(self, words):
        """
        :type words: List[str]
        :rtype: List[str]
        """
        prev_dict =Counter(words[0])
        ans =[words[0]]
        n = len(words)
        for i in range(1,n):
            cur_dict = Counter(words[i])
            if cur_dict!= prev_dict:
                prev_dict= cur_dict
                ans.append(words[i])
        return ans
