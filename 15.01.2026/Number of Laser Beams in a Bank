class Solution(object):
    def numberOfBeams(self, bank):
        c=[]
        for i in range(len(bank)):
            a=bank[i].count("1")
            if a==0:
                continue
            c.append(a)
        if len(c)==1 or len(c)==0:
            return 0
        result=0
        for i in range(0,len(c)):
            try:
                a=c[i]*c[i+1]
                result+=a
            except:
                break
        return result
