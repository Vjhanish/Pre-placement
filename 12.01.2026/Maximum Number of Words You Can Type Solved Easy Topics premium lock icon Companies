class Solution {
    public int canBeTypedWords(String text, String brokenLetters) {
        String[] str = text.split(" ");
        int count = 0;
        for(String st : str){
            boolean canType = true;
            for(char ch : brokenLetters.toCharArray()){
                //broken words
                if(st.indexOf(ch) != -1){
                    System.out.println(st);
                    canType = false;
                    break;
                }
            }if(canType){
                count++;
            }
        }
        return count;
    }
}
